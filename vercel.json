{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/login",
        "dest": "/server.js"
      },
      {
        "src": "/css/(.*)",
        "dest": "/public/css/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }